{
  "name": "junghyeonsu-utils",
  "version": "1.0.1",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "prepack": "yarn build",
    "build": "yarn clean && yarn build:tsc && yarn build:js",
    "build:tsc": "yarn tsc --emitDeclarationOnly",
    "build:js": "node build.js",
    "clean": "rm -rf dist"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./src/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.15.0",
    "esbuild": "^0.17.11",
    "typescript": "^4.9.5"
  }
}
